<div class="page-header">
  <h1>Product Catalog</h1>
</div>

<% @book_list.each do |this_book| %>
  <div class="row">
    <div class="col-md-4">
      <a href="/products/show"><img src= <%=this_book.image_url %> width=300></a>
    </div>
    <div class="col-md-8">
      <h3><a href="/products/show"><%= this_book.name%></a></h3>
      <p class="lead"></p>
      <p><%=this_book.desc %></p>
    </div>
  </div>
<% end %>
